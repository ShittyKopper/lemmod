{
	"name": "lemmod",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"build:script-validator": "ts-json-schema-generator --strict-tuples --path src/script/schema.ts --type Script -o src/script/schema.json && node scripts/generate-validator.js",
		"start": "node out/main.js",
		"fmt": "prettier --write src",
		"lint": "eslint src"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/js-yaml": "^4.0.5",
		"@types/mustache": "^4.2.2",
		"@types/node": "^20.3.0",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"prettier": "^2.8.8",
		"ts-json-schema-generator": "^1.2.0",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"ajv": "^8.12.0",
		"dotenv": "^16.1.4",
		"js-yaml": "^4.1.0",
		"lemmy-js-client": "^0.17.2-rc.24",
		"lru-cache": "^9.1.2",
		"mustache": "^4.2.0",
		"re2": "^1.19.0",
		"sqlite": "^4.2.1",
		"sqlite3": "^5.1.6",
		"tslib": "^2.5.3"
	}
}
